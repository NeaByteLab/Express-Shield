  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^24.0.7",
    "ioredis-mock": "^8.9.0",
    "jest": "^30.0.3",
    "supertest": "^7.1.1",
    "typescript": "^5.8.3"
